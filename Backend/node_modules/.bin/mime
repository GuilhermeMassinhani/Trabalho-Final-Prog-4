#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/55499/Desktop/projetos/prog4Final/Trabalho-Final-Prog-4/Backend/node_modules/mime/node_modules:/mnt/c/Users/55499/Desktop/projetos/prog4Final/Trabalho-Final-Prog-4/Backend/node_modules:/mnt/c/Users/55499/Desktop/projetos/prog4Final/Trabalho-Final-Prog-4/node_modules:/mnt/c/Users/55499/Desktop/projetos/prog4Final/node_modules:/mnt/c/Users/55499/Desktop/projetos/node_modules:/mnt/c/Users/55499/Desktop/node_modules:/mnt/c/Users/55499/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/55499/Desktop/projetos/prog4Final/Trabalho-Final-Prog-4/Backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/55499/Desktop/projetos/prog4Final/Trabalho-Final-Prog-4/Backend/node_modules/mime/node_modules:/mnt/c/Users/55499/Desktop/projetos/prog4Final/Trabalho-Final-Prog-4/Backend/node_modules:/mnt/c/Users/55499/Desktop/projetos/prog4Final/Trabalho-Final-Prog-4/node_modules:/mnt/c/Users/55499/Desktop/projetos/prog4Final/node_modules:/mnt/c/Users/55499/Desktop/projetos/node_modules:/mnt/c/Users/55499/Desktop/node_modules:/mnt/c/Users/55499/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/55499/Desktop/projetos/prog4Final/Trabalho-Final-Prog-4/Backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
